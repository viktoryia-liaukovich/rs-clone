{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": ["warn", "windows"],
        "indent": ["warn", 2],
        "no-var": "error",
        "no-console": "error",
        "import/no-cycle": ["warn"],
        "no-param-reassign": "warn",
    }
}
